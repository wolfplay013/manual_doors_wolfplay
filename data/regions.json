{
    "$schema": "https://raw.githubusercontent.com/ManualForArchipelago/Manual/main/schemas/Manual.regions.schema.json",
	"The Lobby": 	{"connects_to": ["The Hotel 0", "The Mines 0", "Floor Prog 0"], "requires": "", "starting": true},

	"The Hotel 0": 	{"connects_to": ["The Hotel 1"], 				"requires": ""},
	"The Hotel 1": 	{"connects_to": ["The Hotel 2", "The Rooms", "The Outdoors"], 	"requires": "|The Guiding Key:1| OR |The Hotel Key:1|"},
	"The Hotel 2": 	{"connects_to": [], 							"requires": "|The Guiding Key:2| OR |The Hotel Key:2|"},
	"The Mines 0": 	{"connects_to": ["The Mines 1"], 				"requires": "|The Guiding Key:2| OR |The Mines Key:1|"},
	"The Mines 1": 	{"connects_to": ["The Mines 2"], 				"requires": "|The Guiding Key:3| OR |The Mines Key:2|"},
	"The Mines 2": 	{"connects_to": [], 			 				"requires": "|The Guiding Key:4| OR |The Mines Key:3|"},

	"Floor Prog 0": {"connects_to": ["Floor Prog 1", "The Hotel 1", "The Mines 1"], "requires": ""},
	"Floor Prog 1": {"connects_to": ["Floor Prog 2", "The Hotel 2", "The Mines 2"], "requires": "|The Guiding Key:1| OR (|The Hotel Key:1| OR |The Mines Key:2|)"},
	"Floor Prog 2": {"connects_to": [], "requires": "|The Guiding Key:2| OR (|The Hotel Key:2| OR |The Mines Key:2|)"},
	"Subfloor Prog":    {"connects_to": [], "requires": ""},

	"The Rooms": 	{"connects_to": ["The Backdoor", "Subfloor Prog"], "requires": "|Skeleton Key| AND |Lockpick| AND ((|The Curious Key:1| OR |The Rooms Key:1|) AND (|The Guiding Key:1| OR |The Hotel Key:1|))"},
	"The Outdoors": {"connects_to": ["The Backdoor", "Subfloor Prog"], "requires": "|Skeleton Key| AND |Shears| AND ((|The Curious Key:1| OR |The Rooms Key:1|) AND (|The Guiding Key:1| OR |The Hotel Key:1|))"},
	"The Backdoor": {"connects_to": ["Subfloor Prog"], "requires": "|The Curious Key:2| OR |The Backdoor Key:1|"}
}
